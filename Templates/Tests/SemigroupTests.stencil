		}
{% else %}
		property("{{ type.name }} is a {{ protocolName }}") <- forAll { (a: {{ type.name }}, b: {{ type.name }}, c: {{ type.name }}) in
			Law<{{ type.name }}>.isAssociative(a,b,c)
